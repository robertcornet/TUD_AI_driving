<launch>
    <node name="Arduino_0"        pkg="rosserial_python"      type="serial_node.py">
    <param name="port"              type="string"               value="/dev/ttyACM0"/>
    <param name="baud"              type="int"                  value="115200"/>
    </node>

    <node name="Arduino_1"        pkg="rosserial_python"      type="serial_node.py">
    <param name="port"              type="string"               value="/dev/ttyACM1"/>
    <param name="baud"              type="int"                  value="115200"/>
    </node>

    

</launch>


